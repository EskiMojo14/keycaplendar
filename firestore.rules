rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /keysets/{keyset} {
      allow read;
      allow create, update: if request.auth.token.editor == true || request.auth.token.designer == true && request.auth.token.nickname in request.resource.data.designer;
      allow delete: if request.auth.token.editor == true;
    }
  }
}